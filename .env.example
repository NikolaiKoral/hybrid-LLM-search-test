# AI Product Expert Bot Environment Configuration
# Copy this file to .env and fill in your actual values

# Qdrant Vector Database Configuration
# For local Qdrant instance:
# QDRANT_URL=http://localhost:6333
# QDRANT_API_KEY=your_local_api_key_here

# For Qdrant Cloud (recommended for production):
QDRANT_URL=https://your-cluster-id.region.gcp.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_cloud_api_key_here

# Google Cloud Platform Configuration
# Note: Original code had default "its-koral-prod" - replace with your project ID
GOOGLE_CLOUD_PROJECT=your_gcp_project_id_here
GOOGLE_CLOUD_REGION=us-central1

# Gemini API Configuration
# Get this from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Vector Configuration (from config.py - should not be changed unless you know what you're doing)
# MULTIMODAL_EMBEDDING_DIMENSION=1408
# TEXT_EMBEDDING_005_DIMENSION=768
# HOLISTIC_DENSE_VECTOR_DIM=3667
# DEFAULT_DENSE_VECTOR_NAME=holistic_dense
# SPARSE_VECTOR_NAME=minicoil_sparse
# QDRANT_COLLECTION_NAME=ai_product_expert_collection

# Optional: Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Maximum batch size for processing
MAX_BATCH_SIZE=10

# Optional: Maximum image size for downloads (MB)
MAX_IMAGE_SIZE_MB=10

# Feed URL Configuration
# FEED_URL=https://files.channable.com/gOBn8ftRlhqTvgdOkoBnIw==.xml

# Availability options (reference)
# AVAILABILITY_VOCAB=["in_stock", "out_of_stock", "preorder", "backorder"]